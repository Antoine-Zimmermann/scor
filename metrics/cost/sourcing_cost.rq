PREFIX    :   <http://www.ok-fraunhofer.de/2015/04/scor#> 
PREFIX xsd:   <http://www.w3.org/2001/XMLSchema#> 

SELECT (xsd:double((?value1
				   +?value2
				   +?value3
				   +?value4)) AS ?metricResult)
WHERE 
{
	?order :hasMetricCO_5  ?value1 .
	?order :hasMetricCO_6  ?value2 .
	?order :hasMetricCO_7  ?value3 .
	?order :hasMetricCO_8  ?value4 .
}
