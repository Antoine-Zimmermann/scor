<http://eccenca.com/scor/#> rdf:type owl:Ontology ;
							rdfs:label "Supply Chain Operation Reference Vocabulary"^^xsd:string ; 
							dc:date "2014-10-05T12:52:16"^^xsd:dateTime ;
                            rdfs:comment "Supply Chain Operation Reference Vocabulary has the purpose to represent the information about the Supply Chain in order to use this in a Linked Data environement"@en .

# scor Main classes

scor:Process a owl:Class ;
	rdfs:label "Process" ;
	rdfs:comment "A process is a unique activity performed to meet predefined outcomes" .

scor:Metric a owl:Class ;
	rdfs:comment "A metric is a standard for measurement of the performance of a process." ;
	rdfs:label "Metric" .
	
scor:People a owl:Class ;
	rdfs:label "People" ;
	rdfs:comment "Standards for managing talent in the supply chain". 

scor:Practice a owl:Class ;
	rdfs:label "Practice" ;
	rdfs:comment "A practice is a unique way to configure a process or a set of processes". 
	
scor:PerformanceAttribute a owl:Class ;
	rdfs:label "PerformanceAttribute" ;
	rdfs:comment "A performance attribute is a grouping or categorization of metrics used to express a specific strategy". 
	
scor:KeyPerformanceIndicator a owl:Class ;
	rdfs:label "KeyPerformanceIndicator" ;
	rdfs:subClassOf scor:Metric ;
	rdfs:comment "Diagnostics for the overall health of the supply chain. Benchmarking Key Performance Indicators helps establishing realistic targets to support the strategic directions". 

# scor Key Performance Indicators. Metrics of level 1
scor:PerfectOrderFulfillment a owl:Class ;
	rdfs:label "PerfectOrderFulfillment" ;
	rdfs:subClassOf scor:KeyPerformanceIndicator ;
	skos:altLabel "RL.1.1" ;
	rdfs:comment "Delivered complete, on time according the client's specifications of time. Documentation complete. Faultlessly installed".
	
scor:OrderFulfillmentCycleTime a owl:Class ;
	rdfs:label "OrderFulfillmentCycleTime" ;
	rdfs:subClassOf scor:KeyPerformanceIndicator ;
	skos:altLabel "RS.1.1" ;
	rdfs:comment "Order Fulfillment Cycle Time".	
	
scor:UpsideFlexibility a owl:Class ;
	rdfs:label "UpsideFlexibility" ;
	rdfs:subClassOf scor:KeyPerformanceIndicator ;
	skos:altLabel "AG.1.1" ;
	rdfs:comment "Upside Flexibility".	
	
scor:UpsideAdaptability a owl:Class ;
	rdfs:label "UpsideAdaptability" ;
	rdfs:subClassOf scor:KeyPerformanceIndicator ;
	skos:altLabel "AG.1.2" ;
	rdfs:comment "Upside Adaptability".	
	
scor:DownsideAdaptability a owl:Class ;
	rdfs:label "DownsideAdaptability" ;
	rdfs:subClassOf scor:KeyPerformanceIndicator ;
	skos:altLabel "AG.1.3" ;
	rdfs:comment "Downside  Adaptability".
	
scor:OverallValueAtRisk a owl:Class ;
	rdfs:label "OverallValueAtRisk" ;
	rdfs:subClassOf scor:KeyPerformanceIndicator ;
	skos:altLabel "AG.1.4" ;
	rdfs:comment "Overall Value-at-Risk".
	
scor:TotalCostToServe a owl:Class ;
	rdfs:label "TotalCostToServe" ;
	rdfs:subClassOf scor:KeyPerformanceIndicator ;
	skos:altLabel "CO.1.001" ;
	rdfs:comment "The sum of the Supply Chain Cost to deliver products and services to customers".

scor:CashToCashCycleTime a owl:Class ;
	rdfs:label "CashToCashCycleTime" ;
	rdfs:subClassOf scor:KeyPerformanceIndicator ;
	skos:altLabel "AM.1.1" ;
	rdfs:comment "The time it takes for an investment made to flow back into a company after it has been spent for raw materials".

scor:ReturnOnFixedAssets a owl:Class ;
	rdfs:label "ReturnOnFixedAssets" ;
	rdfs:subClassOf scor:KeyPerformanceIndicator ;
	skos:altLabel "AM.1.2." ;
	rdfs:comment "Measures the return an organization receives on its invested capital in supply chain fixed assets. This includes the fixed assets used in Plan, Source, Make,Deliver, and Return".
	
scor:ReturnOnWorkingCapital a owl:Class ;
	rdfs:label "ReturnOnWorkingCapital" ;
	rdfs:subClassOf scor:KeyPerformanceIndicator ;
	skos:altLabel "AM.1.3." ;
	rdfs:comment "Assesses the magnitude of investment relative to a company's working capital position versus the revenue generated from a supply chain.".

# scor Scope
scor:Scope a owl:Class ;
	rdfs:label "Scope" ;
	rdfs:comment "Types of focus measure for performance attributes.".

scor:Internal a owl:Class ;
	rdfs:label "Internal" ;
	rdfs:subClassOf scor:Scope ;
	rdfs:comment "Internal focus for performance attributes".
	
scor:Customer a owl:Class ;
	rdfs:label "Customer" ;
	rdfs:subClassOf scor:Scope ;
	rdfs:comment "Customer focus for performance attributes".
	
scor:Supplier a owl:Class ;
	rdfs:label "Supplier" ;
	rdfs:subClassOf scor:Scope ;
	rdfs:comment "Supplier focus for performance attributes".	
	
# scor Makro Processes

scor:Plan a owl:Class ;
	rdfs:subClassOf scor:Process ;
	rdfs:label "Plan";
	skos:altLabel "sP";
	rdfs:comment "Describes the planning activities associated with operating a supply chain. This includes gathering customer requirements, collecting information on available resources, and balancing requirements and resources to determine planned capabilities and resource gaps. This is followed by identifying the actions required to correct any gaps." ;
    scor:hasProcessID "P" .

scor:Source a owl:Class ;
	rdfs:subClassOf scor:Process ;
	rdfs:label "Source" ;
	skos:altLabel "sS" ;
	rdfs:comment "Order, deliver, receipt and transfer of raw material items, subassemblies, product and/or services" ;
	scor:hasProcessID "S" .

scor:Make a owl:Class ;
	rdfs:subClassOf scor:Process ;
	rdfs:label "Make" ;
	skos:altLabel "sM" ;
	rdfs:comment "Adds value to products through mixing, separating, forming, machining, and chemical processes" ;
	scor:hasProcessID "M" .

scor:Deliver a owl:Class ;
	rdfs:subClassOf scor:Process ;
	rdfs:label "Deliver" ;
	rdfs:comment "Performs customer-facing order management and order fulfillment activities" ;
	skos:altLabel "sD" ;
	scor:hasProcessID "D" .

scor:Return a owl:Class ;
	rdfs:subClassOf scor:Process ;
	rdfs:label "Return" ;
	skos:altLabel "sR" ;
	rdfs:comment "Moves material from a customer back through the supply chain to address defects in product, ordering, or manufacturing, or to perform upkeep activities" ;
	scor:hasProcessID "R" .

scor:Enable a owl:Class ;
	rdfs:subClassOf scor:Process ;
	rdfs:label "Enable" ;
	skos:altLabel "sE";
	rdfs:comment "Establishing, maintaining and monitoring information, relationships, resources, assets, business rules, compliance and contracts required to operate the supply chain";
	scor:hasProcessID "E" .

